import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;



class Terson {
		
		String prenom;
		Long[] num;
		
		Long[] getNumbers() {
			return this.num;
		}
		
		Terson() {
			
		}
		Terson(String a , Long[] num) {
			this.prenom = a ;
			this.num = num;
		}
		
	
		public static void main(String[] args ) {
			
		
			Terson a = new Terson("Hamza", new Long[]{1L,5L,8L,9L});
			Terson b = new Terson("Anas", new Long[]{1L,5L,8L,9L});
			List<Terson> t = new ArrayList();
			t.add(a);
			t.add(b);
			


			Arrays.asList(t).stream().map(x -> x.)
		}
	}